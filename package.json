{
    "name": "reselect-lang",
    "displayName": "Reselect Language",
    "description": "Syntax highlighting for Reselect",
    "version": "1.2.0",
    "license": "GPL-3.0-or-later",
    "icon": "images/icon.png",
    "publisher": "digifox03",
    "homepage": "https://github.com/Digifox03/reselect-vscode",
    "bugs": {
        "url": "https://github.com/Digifox03/reselect-vscode/issues",
        "email": "digifox03@protonmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Digifox03/reselect-vscode"
    },
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/extension.js",
    "activationEvents": [
        "onLanguage:reselect"
    ],
    "contributes": {
        "languages": [{
            "id": "reselect",
            "aliases": ["Reselect", "reselect"],
            "extensions": [".reselect"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "reselect",
            "scopeName": "source.reselect",
            "path": "./syntaxes/reselect.tmLanguage.json"
        }]
    },
    "scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.2.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"typescript": "^4.5.4"
	}
}